{
    "input_model": {
        "type": "HfModel",
        "model_path": "google-bert/bert-base-multilingual-cased",
        "task": "fill-mask",
        "load_kwargs": { "attn_implementation": "eager" },
        "io_config": {
            "input_names": [
                "input_ids",
                "attention_mask",
                "token_type_ids"
            ],
            "input_shapes": [
                [1, 128],
                [1, 128],
                [1, 128]
            ],
            "input_types": ["int64", "int64", "int64"],
            "output_names": ["logits"],
            "output_shapes": [
                [1, 128, 119547]
            ],
            "output_types": ["float32"]
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "accelerators": [ { "device": "npu", "execution_providers": [ "QNNExecutionProvider" ] } ]
        }
    },
    "data_configs": [
        {
            "name": "quantization_data_config",
            "user_script": "mlm_user_script.py",
            "load_dataset_config": {
                "type": "quantization_dataset",
                "model_name": "google-bert/bert-base-multilingual-cased",
                "start": 0,
                "end": 10
            },
            "dataloader_config": { "type": "no_auto_batch_dataloader" }
        },
        {
            "name": "mlm_data_config",
            "user_script": "mlm_user_script.py",
            "load_dataset_config": {
                "type": "mlm_dataset",
                "model_name": "google-bert/bert-base-multilingual-cased",
                "start": 10,
                "end": 20
            },
            "dataloader_config": { "type": "no_auto_batch_dataloader" }
        }
    ],
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "perplexity",
                    "type": "custom",
                    "data_config": "mlm_data_config",
                    "sub_types": [ { "name": "perplexity_score", "priority": 1, "higher_is_better": false } ],
                    "user_config": {
                        "user_script": "mlm_user_script.py",
                        "evaluate_func": "eval_perplexity"
                    }
                },
                {
                    "name": "latency",
                    "type": "latency",
                    "data_config": "mlm_data_config",
                    "sub_types": [ { "name": "avg", "priority": 2 } ]
                },
                {
                    "name": "latency_cpu",
                    "type": "latency",
                    "data_config": "mlm_data_config",
                    "sub_types": [ { "name": "avg", "priority": 3 } ],
                    "inference_settings": { "onnx": { "execution_provider": "CPUExecutionProvider" } }
                }
            ]
        }
    },
    "passes": {
        "conversion": { "type": "OnnxConversion", "target_opset": 17 },
        "dynamic_shape_to_fixed": {
            "type": "DynamicToFixedShape",
            "dim_param": [ "batch_size", "sequence_length" ],
            "dim_value": [ 1, 128 ]
        },
        "surgery": { "type": "GraphSurgeries", "surgeries": [ { "surgeon": "ReplaceAttentionMaskValue" } ] },
        "qnn_preprocess": { "type": "QNNPreprocess" },
        "quantization": {
            "type": "OnnxStaticQuantization",
            "data_config": "quantization_data_config",
            "activation_type": "QUInt16",
            "weight_type": "QUInt8"
        }
    },
    "evaluate_input_model": false,
    "host": "local_system",
    "target": "local_system",
    "cache_dir": "cache",
    "output_dir": "models/google-bert_bert-base-multilingual-cased"
}
